    "@sparticuz/chromium": "^131.0.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "express": "^4.19.2",
    "express-winston": "^4.2.0",
    "googleapis": "^123.0.0",
    "handlebars": "^4.7.8",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql": "^2.18.1",
    "mysql2": "^3.6.2",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.10.0",
    "nodemon": "^3.0.1",
    "phone": "^3.1.42",
    "puppeteer": "^24.1.1",
    "puppeteer-core": "^24.1.1",
    "uuid": "^11.0.5",
    "validator": "^13.11.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "winston-mongodb": "^7.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
